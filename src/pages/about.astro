---
import background from "../assets/background.svg";
import Navigation from "../components/Navigation.astro";
const name = "Lucas Audart";
---

<div>
  <Navigation />
  <img id="background" src={background.src} alt="" fetchpriority="high" />
  About {name}
</div>

<style>
  #background {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: -1;
    filter: blur(100px);
  }
</style>
